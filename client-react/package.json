    "react-app-rewired": "^2.1.8",
    "redux-mock-store": "^1.5.4",
    "typescript": "^4.1.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "import-helpers"
    ],
    "rules": {
      "import-helpers/order-imports": [
        "warn",
        {
          "newlinesBetween": "always",
          "groups": [
            "/^react/",
            "module",
            "/^~/",
            [
              "parent",
              "sibling",
              "index"
            ]
          ],
          "alphabetize": {
            "order": "asc",
            "ignoreCase": true
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
